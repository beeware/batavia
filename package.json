{
  "name": "@pybee/batavia",
  "version": "3.4.0-dev.17",
  "description": "A Javascript implementation of the Python virtual machine.",
  "main": "batavia/batavia.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "python compile_stdlib.py",
