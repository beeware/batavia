    "build": "node_modules/.bin/webpack --progress",
    "watch": "node_modules/.bin/webpack --progress --watch",
    "serve": "node_modules/.bin/webpack-dev-server --inline --hot --port 3000"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pybee/batavia.git"
  },
  "keywords": [
    "python"
  ],
